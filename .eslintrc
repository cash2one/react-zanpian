{
  "extends": ["standard", "eslint:recommended", "plugin:react/recommended"],
  "globals": {
    "describe": true,
    "beforeEach": true,
    "afterEach": true,
    "after": true,
    "it": true,
    "ArriveFooter": true,
    "__CLIENT__": true
  },
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": 1,
    "no-console": "off",
    "no-unused-vars": 0,
    "react/display-name": [{ "ignoreTranspilerName": false }]
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  }
}
